                                        "prev": {
                                            "href": "string"
                                        },
                                        "next": {
                                            "href": "string"
                                        },
                                        "last": {
                                            "href": "string"
                                        }
                                    },
                                    "currency": "string",
                                    "page": {
                                        "size": 10,
                                        "total_items": 0,
                                        "total_pages": 0,
                                        "number": 0
                                    },
                                    "page_total": {
                                        "amount": 0
                                    },
                                    "total": {
                                        "amount": 0
                                    },
                                    "period": {
