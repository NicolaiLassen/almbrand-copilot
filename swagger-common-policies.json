                                            "subtitle": "Dækker færdselsuheld, brand og tyveri og røveri af bilen. Kasko dækker også følgende. Selvrisiko",
                                            "covered": true,
                                            "properties": [
                                                {
                                                    "propertyType": "string",
                                                    "title": "string",
                                                    "subtitle": "string"
                                                }
                                            ],
                                            "sub_coverages": [
                                                {
                                                    "title": "string",
                                                    "subtitle": "string",
                                                    "properties": [
                                                        {
                                                            "propertyType": "string",
                                                            "title": "string",
                                                            "subtitle": "string"
                                                        }
                                                    ]
                                                }
                                            ]
                                        }
                                    ],
                                    "parties": [
                                        {
                                            "name": "string",
                                            "address": "string"
                                        }
                                    ]
                                }
                            }
                        }
                    },
                    "401": {
                        "description": "Unauthorized",
                        "headers": {
                            "www-authenticate": {
                                "description": "scope=[ required scope of the access token to access the requested resource ]",
                                "schema": {
                                    "type": "string"
                                }
                            }
                        },
                        "content": {
                            "application/problem+json": {
                                "schema": {
                                    "$ref": "#/components/schemas/Problem"
                                },
                                "example": {
                                    "type": "string",
                                    "title": "string",
                                    "status": 0,
                                    "detail": "string",
                                    "instance": "string"
                                }
                            }
                        }
                    },
                    "404": {
                        "description": "NotFound",
                        "content": {
                            "application/problem+json": {
                                "schema": {
                                    "$ref": "#/components/schemas/Problem"
                                },
                                "example": {
                                    "type": "string",
                                    "title": "string",
                                    "status": 0,
                                    "detail": "string",
                                    "instance": "string"
                                }
                            }
                        }
                    },
                    "500": {
                        "description": "Internal Server Error",
                        "content": {
                            "application/problem+json": {
                                "schema": {
                                    "$ref": "#/components/schemas/Problem"
                                },
                                "example": {
                                    "type": "string",
                                    "title": "string",
                                    "status": 0,
                                    "detail": "string",
                                    "instance": "string"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/{policy_id}/pdf": {
            "get": {
                "tags": [
                    "Document",
                    "Policy",
                    "Party"
                ],
                "summary": "Get document",
                "description": "Get a document (pdf) by policy_id, for the customer to which the bearer token in Authorization header is issued. If the policy_id is not belonging or accessible to the customer a problem occurs.\n",
                "operationId": "fetchPolicyPdfById",
                "parameters": [
                    {
                        "name": "policy_id",
                        "in": "path",
                        "description": "Unique id for a policy.",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "content": {
                            "application/pdf": {
                                "schema": {
                                    "type": "object"
                                },
                                "examples": {
                                    "default": {
                                        "value": null
                                    }
                                }
                            }
                        }
                    },
                    "400": {
                        "description": "Problem Occured",
                        "content": {
                            "application/problem+json": {
                                "schema": {
                                    "$ref": "#/components/schemas/Problem"
                                },
                                "example": {
                                    "type": "string",
                                    "title": "string",
                                    "status": 0,
                                    "detail": "string",
                                    "instance": "string"
                                }
                            }
                        }
                    },
                    "500": {
                        "description": "Problem Occured",
                        "content": {
                            "application/problem+json": {
                                "schema": {
                                    "$ref": "#/components/schemas/Problem"
                                },
                                "example": {
                                    "type": "string",
                                    "title": "string",
                                    "status": 0,
                                    "detail": "string",
                                    "instance": "string"
                                }
                            }
                        }
                    }
                }
            }
        }
    },
    "components": {
        "schemas": {
            "Search": {
                "type": "object",
                "properties": {
                    "include_own": {
                        "type": "boolean",
                        "description": "Indicates that the policies owned by the party should be returned in response."
                    },
                    "page": {
                        "type": "integer",
                        "description": "The page offset",
                        "format": "int32"
                    },
                    "size": {
                        "type": "integer",
                        "description": "The page size",
                        "format": "int32"
                    }
                }
            },
            "PolicyPage": {
                "type": "object",
                "properties": {
                    "_links": {
                        "$ref": "#/components/schemas/Common.Links"
                    },
                    "currency": {
                        "type": "string"
                    },
                    "page": {
                        "$ref": "#/components/schemas/Pagination"
                    },
                    "page_total": {
                        "$ref": "#/components/schemas/Total"
                    },
                    "total": {
                        "$ref": "#/components/schemas/Total"
                    },
                    "period": {
                        "$ref": "#/components/schemas/Period"
                    },
                    "policies": {
                        "type": "array",
                        "items": {
                            "$ref": "#/components/schemas/PolicySummary"
                        }
                    },
                    "household_relationships": {
                        "type": "array",
                        "items": {
                            "$ref": "#/components/schemas/HouseholdRelationship"
                        }
                    }
                },
                "description": "A page of policies. The total price is calculated as the sum price of all policies on the page. It does NOT cover all policies for the customer."
            },
            "Total": {
                "properties": {
                    "amount": {
                        "type": "number"
                    }
                },
                "description": "Totals for the page and periodx"
            },
            "Common.Links": {
                "properties": {
                    "self": {
                        "$ref": "#/components/schemas/Common.Link"
                    },
                    "first": {
                        "$ref": "#/components/schemas/Common.Link"
                    },
                    "prev": {
                        "$ref": "#/components/schemas/Common.Link"
                    },
                    "next": {
                        "$ref": "#/components/schemas/Common.Link"
                    },
                    "last": {
                        "$ref": "#/components/schemas/Common.Link"
                    }
                }
            },
            "Common.Link": {
                "properties": {
                    "href": {
                        "type": "string",
                        "format": "url"
                    }
                }
            },
            "Period": {
                "properties": {
                    "start_at": {
                        "type": "string",
                        "format": "date"
                    },
                    "end_at": {
                        "type": "string",
                        "format": "date"
                    }
                },
                "description": "The period considered and for which the sum of charges is calculated"
            },
            "DateDescription": {
                "properties": {
                    "title": {
                        "type": "string"
                    },
                    "date": {
                        "type": "string",
                        "format": "date"
                    }
                },
                "description": "A date with a label"
            },
            "PolicySummary": {
                "properties": {
                    "id": {
                        "type": "string",
                        "description": "Identification of a policy eg. the policy number stated on the policy document signed by the customer.",
                        "example": "106585963"
                    },
                    "policy_number": {
                        "type": "string",
                        "example": "106585963"
                    },
                    "type": {
                        "$ref": "#/components/schemas/Type"
                    },
                    "status": {
                        "$ref": "#/components/schemas/Status"
                    },
                    "group": {
                        "$ref": "#/components/schemas/Group"
                    },
                    "url": {
                        "type": "string",
                        "description": "RESTful urls for policy resource",
                        "example": "https://api.almbrand.dk/common/policies/106585963"
                    },
                    "title": {
                        "type": "string",
                        "description": "title for the policy",
                        "example": "Bilforsikring"
                    },
                    "subtitle": {
                        "type": "string",
                        "description": "subtitle for the policy.",
                        "example": "Fiat 500 AB12645"
                    },
                    "price": {
                        "$ref": "#/components/schemas/Price"
                    },
                    "start": {
                        "$ref": "#/components/schemas/DateDescription"
                    },
                    "end": {
                        "$ref": "#/components/schemas/DateDescription"
                    }
                }
            },
            "Policy": {
                "type": "object",
                "properties": {
                    "id": {
                        "type": "string",
                        "example": "106590727"
                    },
                    "policy_number": {
                        "type": "string",
                        "example": "321654987"
                    },
                    "type": {
                        "$ref": "#/components/schemas/Type"
                    },
                    "status": {
                        "$ref": "#/components/schemas/Status"
                    },
                    "group": {
                        "$ref": "#/components/schemas/Group"
                    },
                    "url": {
                        "type": "string",
                        "example": "https://api.almbrand.dk/common/policies/106590727"
                    },
                    "start": {
                        "$ref": "#/components/schemas/DateDescription"
                    },
                    "end": {
                        "$ref": "#/components/schemas/DateDescription"
                    },
                    "customer_id": {
                        "type": "string",
                        "example": "1234567890"
                    },
                    "currency": {
                        "type": "string",
                        "example": "DKK"
                    },
                    "objects": {
                        "type": "array",
                        "items": {
                            "$ref": "#/components/schemas/InsurableObject"
                        }
                    },
                    "title": {
                        "type": "string",
                        "description": "title for the policy",
                        "example": "Bilforsikring"
                    },
                    "subtitle": {
                        "type": "string",
                        "description": "subtitle for the policy.",
                        "example": "Fiat 500 AB12645"
                    },
                    "price": {
                        "$ref": "#/components/schemas/Price"
                    },
                    "documents": {
                        "$ref": "#/components/schemas/DocumentList"
                    },
                    "coverages": {
                        "$ref": "#/components/schemas/CoverageList"
                    },
                    "parties": {
                        "$ref": "#/components/schemas/PartyList"
                    }
                }
            },
            "PartyList": {
                "type": "array",
                "items": {
                    "$ref": "#/components/schemas/Party"
                },
                "description": "A list of parties (people or organizations) related to the policy"
            },
            "Party": {
                "type": "object",
                "properties": {
                    "name": {
                        "type": "string"
                    },
                    "address": {
                        "type": "string"
                    }
                }
            },
            "CoverageList": {
                "type": "array",
                "items": {
                    "$ref": "#/components/schemas/Coverage"
                },
                "description": "A list of coverages related to the policy"
            },
            "Price": {
                "properties": {
                    "title": {
                        "type": "string",
                        "example": "årlig"
                    },
                    "amount": {
                        "type": "number",
                        "example": 1234.55
                    },
                    "charged": {
                        "$ref": "#/components/schemas/Charged"
                    },
                    "unit": {
                        "type": "string",
                        "x-extensible-enum": [
                            "dk",
                            "km"
                        ]
                    }
                }
            },
            "Coverage": {
                "properties": {
                    "url": {
                        "type": "string"
                    },
                    "title": {
                        "type": "string",
                        "example": "Kasko"
                    },
                    "subtitle": {
                        "type": "string",
                        "example": "Dækker færdselsuheld, brand og tyveri og røveri af bilen. Kasko dækker også følgende. Selvrisiko"
                    },
                    "covered": {
                        "type": "boolean",
                        "description": "Indicates if the coverage is included or not",
                        "example": true
                    },
                    "properties": {
                        "$ref": "#/components/schemas/CoveragePropertyList"
                    },
                    "sub_coverages": {
                        "$ref": "#/components/schemas/SubcoverageList"
                    }
                }
            },
            "SubcoverageList": {
                "type": "array",
                "items": {
                    "$ref": "#/components/schemas/Subcoverage"
                },
                "description": "List of sub coverages"
            },
            "Subcoverage": {
                "properties": {
                    "title": {
                        "type": "string"
                    },
                    "subtitle": {
                        "type": "string"
                    },
                    "properties": {
                        "$ref": "#/components/schemas/CoveragePropertyList"
                    }
                }
            },
            "DocumentList": {
                "type": "array",
                "items": {
                    "$ref": "#/components/schemas/Document"
                },
                "description": "A list of documents associated with the policy"
            },
            "Document": {
                "properties": {
                    "url": {
                        "type": "string",
                        "example": "http://common/policies/106590727/pdf"
                    },
                    "title": {
                        "type": "string",
                        "example": "Vilkår og betingelser"
                    },
                    "subtitle": {
                        "type": "string",
                        "example": "Betingelserne beskriver detaljeret, hvad forsikringen dækker og ikke dækker."
                    },
                    "content_type": {
                        "type": "string",
                        "example": "application/pdf"
                    }
                },
                "description": "A single document related to the policy eg. terms and conditions or the policy itself."
            },
            "Group": {
                "type": "object",
                "properties": {
                    "title": {
                        "type": "string",
                        "example": "Biler og køretøjer"
                    },
                    "type": {
                        "type": "string",
                        "example": "vehicles",
                        "x-extensible-enum": [
                            "buildings",
                            "operations",
                            "employees",
                            "vehicles",
                            "housing",
                            "health",
                            "family",
                            "other"
                        ]
                    }
                },
                "description": "The group in which the policy is categorized."
            },
            "Status": {
                "type": "object",
                "properties": {
                    "title": {
                        "type": "string",
                        "example": "Aktiv"
                    },
                    "subtitle": {
                        "type": "string",
                        "example": "Policen er aktiv."
                    },
                    "type": {
                        "enum": [
                            "ACTIVE",
                            "PENDING",
                            "OFFER",
                            "CANCELLED"
                        ],
                        "type": "string",
                        "example": "ACTIVE"
                    }
                },
                "description": "The current status of the policy."
            },
            "Charged": {
                "enum": [
                    "EVERY_MONTH",
                    "EVERY_TWO_MONTHS",
                    "EVERY_THREE_MONTHS",
                    "EVERY_FOUR_MONTHS",
                    "EVERY_SIX_MONTHS",
                    "EVERY_YEAR",
                    "ONCE"
                ],
                "type": "string",
                "description": "How often is the insurance premium charged",
                "example": "EVERY_MONTH"
            },
            "InsurableObject": {
                "type": "object",
                "properties": {
                    "title": {
                        "type": "string",
                        "example": "Fiat 500"
                    },
                    "subtitle": {
                        "type": "string",
                        "example": "AB 123456"
                    },
                    "properties": {
                        "$ref": "#/components/schemas/InsurableObjectPropertyList"
                    }
                },
                "description": "The Insurable Object is an item which may be included or excluded from an insurance coverage or policy, either as the object for which possible damages to the object or loss of the object is insured, or as the object for which damages caused by the object are insured"
            },
            "InsurableObjectPropertyList": {
                "type": "array",
                "items": {
                    "$ref": "#/components/schemas/InsurableObjectProperty"
                }
            },
            "HouseholdRelationship": {
                "type": "object",
                "properties": {
                    "for_name": {
                        "type": "string"
                    },
                    "last_name": {
                        "type": "string"
                    }
                },
                "description": "Holding the information for household persons"
            },
            "InsurableObjectProperty": {
                "required": [
                    "propertyType"
                ],
                "type": "object",
                "properties": {
                    "propertyType": {
                        "type": "string"
                    },
                    "title": {
                        "type": "string"
                    },
                    "subtitle": {
                        "type": "string"
                    }
                },
                "discriminator": {
                    "propertyName": "propertyType",
                    "mapping": {
                        "limitList": "#/components/schemas/LimitList",
                        "deductible": "#/components/schemas/Deductible",
                        "total": "#/components/schemas/Total",
                        "titles": "#/components/schemas/Titles"
                    }
                }
            },
            "LimitList": {
                "allOf": [
                    {
                        "$ref": "#/components/schemas/InsurableObjectProperty"
                    },
                    {
                        "type": "object",
                        "properties": {
                            "limits": {
                                "type": "array",
                                "items": {
                                    "$ref": "#/components/schemas/Limit"
                                }
                            }
                        }
                    }
                ],
                "description": "List of limits"
            },
            "Limit": {
                "properties": {
