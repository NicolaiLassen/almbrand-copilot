    "paths": {
        "/": {
            "get": {
                "tags": [
                    "Policy"
                ],
                "summary": "Get Policies.",
                "description": "Get all policies for a customer. If the parameter include_own = true the response will only include policy owned by the customer. Otherwise all policies accessible for the customer will be included. The customer is designated through the authorization bearer token.\n",
                "operationId": "fetchPolicies",
